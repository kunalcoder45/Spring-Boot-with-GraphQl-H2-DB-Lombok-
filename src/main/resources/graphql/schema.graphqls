type Mutation{
    createBook(book: BookInput):Book
}

type Query {

    allBooks:[Book]
    getBook(bookId:Int):Book
}

type Book {

    id:ID!
    title:String
    desc:String
    price:Float
    author:String
    pages:Int
}

input BookInput {

    title:String
    desc:String
    price:Float
    author:String
    pages:Int
}